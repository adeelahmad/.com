{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@fontsource/crimson-pro": "4.5.6",
    "@fontsource/inter": "4.5.7",
    "@fontsource/playfair-display": "4.5.6",
    "@fontsource/space-mono": "4.5.5",
    "@juggle/resize-observer": "3.3.1",
    "@radix-ui/react-dialog": "0.1.7",
    "@radix-ui/react-icons": "1.1.0",
    "@radix-ui/react-navigation-menu": "0.1.2",
    "@radix-ui/react-toggle": "0.1.4",
    "@segment/snippet": "4.15.3",
    "@stitches/react": "1.2.7",
    "@types/he": "1.1.2",
    "@types/simple-react-lightbox": "3.6.1",
    "@u-wave/react-vimeo": "0.9.5",
    "add": "2.0.6",
    "axios": "0.26.1",
    "ethers": "5.6.2",
    "graphql": "16.3.0",
    "he": "1.2.0",
    "next": "12.1.1",
    "next-mdx-remote": "4.0.2",
    "next-themes": "0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hotkeys-hook": "3.4.4",
    "react-masonry-css": "1.0.16",
    "react-use-measure": "2.1.1",
    "swr": "1.2.2",
    "wagmi": "0.2.21",
    "yarn": "1.22.18"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
