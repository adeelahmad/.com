{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@capsizecss/core": "3.0.0",
    "@capsizecss/metrics": "0.2.0",
    "@capsizecss/vanilla-extract": "1.0.0",
    "@dessert-box/react": "0.4.0",
    "@mdx-js/loader": "2.1.2",
    "@next/mdx": "12.2.0",
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-collapsible": "0.1.6",
    "@radix-ui/react-dialog": "0.1.7",
    "@radix-ui/react-icons": "1.1.1",
    "@radix-ui/react-navigation-menu": "0.1.2",
    "@rainbow-me/rainbowkit": "0.4.1",
    "@u-wave/react-vimeo": "0.9.8",
    "@vanilla-extract/babel-plugin": "1.1.7",
    "@vanilla-extract/css": "1.7.2",
    "@vanilla-extract/next-plugin": "2.0.2",
    "@vanilla-extract/recipes": "0.2.5",
    "@vanilla-extract/sprinkles": "1.4.1",
    "axios": "0.27.2",
    "copy-data-to-clipboard": "1.0.0",
    "date-fns": "2.28.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "ethers": "5.6.9",
    "graphql": "16.5.0",
    "gray-matter": "4.0.3",
    "he": "1.2.0",
    "next": "12.2.0",
    "next-mdx-remote": "4.0.3",
    "next-pwa": "5.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "3.4.6",
    "react-masonry-css": "1.0.16",
    "react-remark": "2.1.0",
    "react-static-tweets": "0.8.0",
    "react-use-measure": "2.1.1",
    "rehype-raw": "6.1.1",
    "static-tweets": "0.8.0",
    "swr": "1.3.0",
    "wagmi": "0.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.1.4",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
